#: ../panel-plugin/verve.c:98
#, c-format
msgid "Could not execute command (exit status %d)"
msgstr "Չյաջողուեց գործադրել հրամանը (ելային վիճակ %d)"

#. Generate error message
#: ../panel-plugin/verve-plugin.c:392
msgid "Could not execute command:"
msgstr "Հնարաւոր չէ գործադրել հրամանը"

#. Create properties dialog
#: ../panel-plugin/verve-plugin.c:1137
msgid "Verve"
msgstr "Գունեղ"

#: ../panel-plugin/verve-plugin.c:1160
msgid "General"
msgstr "Հիմնական"

#. Frame for appearance settings
#: ../panel-plugin/verve-plugin.c:1165
msgid "Appearance"
msgstr "Տեսք"

#. Plugin size label
#: ../panel-plugin/verve-plugin.c:1181
msgid "Width (in chars):"
msgstr "Լայնութիւն (գրանշաններով)․"

#. Plugin label label
#: ../panel-plugin/verve-plugin.c:1206
msgid "Label:"
msgstr "Պիտակ․"

#. Frame for color settings
#: ../panel-plugin/verve-plugin.c:1223
msgid "Colors"
msgstr "Գոյներ"

#. Plugin background color label
#: ../panel-plugin/verve-plugin.c:1239
msgid "Background color:"
msgstr "Յետնապատկերի գոյն․"

#. Plugin foreground color label
#: ../panel-plugin/verve-plugin.c:1265
msgid "Text color:"
msgstr "Գրոյթի գոյն․"

#. Frame for behaviour settings
#: ../panel-plugin/verve-plugin.c:1286
msgid "History"
msgstr "Պատմութիւն"

#. History length label
#: ../panel-plugin/verve-plugin.c:1302
msgid "Number of saved history items:"
msgstr "Պատմութեան պահուախ տարրերի քանակ․"

#. Second tab
#: ../panel-plugin/verve-plugin.c:1322 ../panel-plugin/verve-plugin.c:1326
msgid "Behaviour"
msgstr "Վարքագիծ"

#. Pattern types frame label
#: ../panel-plugin/verve-plugin.c:1335
msgid "Enable support for:"
msgstr "Թոյլատրել աջակցութիւն․"

#. Command type: URL
#: ../panel-plugin/verve-plugin.c:1341
msgid "URLs (http/https/ftp/ftps)"
msgstr "URL֊ներ (http/https/ftp/ftps)"

#. Command type: email address
#: ../panel-plugin/verve-plugin.c:1349
msgid "Email addresses"
msgstr "Էլ․ հասցէներ"

#. Command type: directory path
#: ../panel-plugin/verve-plugin.c:1357
msgid "Directory paths"
msgstr "Նշարանների ուղիներ"

#: ../panel-plugin/verve-plugin.c:1370
msgid "Expand variables with wordexp"
msgstr "Ընդարձակել փոփոխականնեերը աւգտուելով wordexp"

#. Command type: !bang
#: ../panel-plugin/verve-plugin.c:1380
msgid "DuckDuckGo queries (starting with !)"
msgstr "DuckDuckGo հարցումներ (սկսուում են ! ֊ով)"

#. Command type: I'm feeling ducky (\)
#: ../panel-plugin/verve-plugin.c:1388
msgid "DuckDuckGo queries (starting with \\)"
msgstr "DuckDuckGo հարցումներ (սկսուում են \\֊ով)"

#. Fallback if the above don't match
#: ../panel-plugin/verve-plugin.c:1396
msgid "If the above patterns don't match:"
msgstr "Եթե վերին նախշերը չեն համընկնում․"

#. Smart bookmark radio button
#: ../panel-plugin/verve-plugin.c:1402
msgid "Use smart bookmark URL"
msgstr "Աւգտագործել խելացի էջանիշի URL"

#. Executable command radio button (smart bookmark off)
#: ../panel-plugin/verve-plugin.c:1425
msgid "Run as executable command"
msgstr "Գործարկել որպէս գործադիր հրաման"

#: ../panel-plugin/verve-plugin.c:1437
msgid ""
"Run command with $SHELL -i -c\n"
"(enables alias and variable expansion)"
msgstr "Գործարկել հրամանը կիրառելով $SHELL -i -c\n(թոյլատրում այլանուն եւ փոփոխական ընդարձակում)"

#. vim:set expandtab sts=2 ts=2 sw=2:
